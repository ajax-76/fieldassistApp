@model AllErrors

@if (Model == null)
{
    <br />
    <br />
    <div class="input" style="color:red;">
        <h1 style="text-align:center;"><span class="label label-default">Upload For Passive Checks (Beat and Hierarchy)</span></h1>
        <br />
        @using (Html.BeginForm("Upload", "Upload", FormMethod.Post , new { enctype = "multipart/form-data" }))
        {
            <input type="file" name="file" />
            <br />
            <p><span class="label label-warning"> Upload File in xlsx Format </span> </p>
            <br />
            <input id="wait" class="btn-success" type="submit" value="upload" />

        }
    </div>
}
<br />

@if (Model != null)

{
    <h1 style="text-align:center;"><span class="label label-info">All Errors and Warnings List</span></h1>
    <br />

    if (Model.Warning != null)
    {
        <h2 style="text-align:center" ;><span class="label label-warning">Warnings</span></h2>
        <table class="table">
            <thead>
                <tr>
                    <th></th>
                    <th><span class="label label-warning">Warnings</span></th>
                    <th><span class="label label-warning"> Field</span></th>
                    <th><span class="label label-warning">Row</span></th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model.Warning)
                {
                    <tr class="label-warning">
                        <td><img src="data:image/png;base64," /></td>
                        <td>@Html.DisplayFor(modelItem => item.Comments)</td>
                        <td>@Html.DisplayFor(modelItem => item.Field)</td>
                        <td>@Html.DisplayFor(modelItem => item.Row)</td>
                    </tr>
                }
            </tbody>
        </table>
        <p><span class="label label-success">Do you Wish To continue</span></p>
        <button class="alert-warning" id="hide">No</button>
    }


    <button class="alert-danger" id="showError">show Error</button>
    <div id="error">
        <h3 style="text-align:center" ;><span class="label label-danger">Errors</span></h3>
        <table class="table">
            <thead>
                <tr>
                    <th></th>
                    <th><span class="label label-danger">Error Type</span></th>
                    <th><span class="label label-danger">Field1</span></th>
                    <th><span class="label label-danger">Field2</span></th>
                    <th><span class="label label-danger">Error Description</span></th>
                    @if (Model.ShowHeader != null)
                    {
                        <th><span class="label label-danger">IncorrectHeaderList</span></th>
                    }
                    <th><span class="label label-danger">CheckRowForField_2</span></th>

                </tr>
            </thead>
            <tbody>

                @foreach (var item in Model.Error)
                {


                    <tr class="label-danger" style="color:snow;">
                        <td><span class="glyphicons glyphicons-skull"><img src="data:image/png;base64," /></span></td>
                        <td>@Html.DisplayFor(modeItem => item.ErrorType)</td>
                        <td>@Html.DisplayFor(modeItem => item.Field_1)</td>
                        <td>@Html.DisplayFor(modeItem => item.Field_2)</td>
                        <td>@Html.DisplayFor(modeItem => item.ErrorComments)</td>
                        @if (Model.ShowHeader != null)
                        {
                            <td>
                                <table>

                                    @foreach (var key in Model.ShowHeader.IncorrectHeaderList)
                                    {
                                        <tr style="color:snow;"><td>@key</td></tr>
                                    }

                                </table>
                            </td>
                        }
                        <td>@Html.DisplayFor(modelItem => item.Row)</td>

                    </tr>




                }
            </tbody>
        </table>
    </div>

}
<script type="text/javascript" src="http://code.jquery.com/jquery.js"></script>
<script>

    $('#wait').click(function () {
        $('.input').text("Wait for the process to be completed");
    });
    $('#error').hide('fast');
        $('#show').click(function () {
            $('#error').show();
            $('#show').hide();
            $('#hide').hide();
            $('#hide2').hide();
            $('p').hide();
        });
        $('#hide').click(function () {
            $('#error').hide();
            $('#show').hide();
            $('#hide').hide();
            $('#hide2').hide();
            $('p').hide();
            $('#showError').hide();
        });
        $('#hide2').click(function () {
            $('#error').hide();
            $('#show').hide();
            $('#hide').hide();
            $('#hide2').hide();
            $('p').hide();
            $('#showError').hide();
        });
        $('#showError').click(function () {
            $('#error').show();
            $('#show').hide();
            $('#hide').hide();
            $('#hide2').hide();
            $('p').hide();
            $('#showError').hide();
        });
</script>
